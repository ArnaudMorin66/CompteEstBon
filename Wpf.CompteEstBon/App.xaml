<Application x:Class="CompteEstBon.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Luna"
             xmlns:Themes1="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero2"
             xmlns:local="clr-namespace:CompteEstBon"
             xmlns:syncfusion="http://schemas.syncfusion.com/wpf"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <LinearGradientBrush x:Key="CebLinearGradientBrush1" StartPoint="0,0" EndPoint="0,1">
            <GradientStop Offset="0.0" Color="LightGreen" />
            <GradientStop Offset="1.0" Color="Navy" />
        </LinearGradientBrush>
        <LinearGradientBrush x:Key="CebLinearGradientBrush2" StartPoint="0.5,0" EndPoint="0.5,1">
            <GradientStop Offset="0.0" Color="Green" />
            <GradientStop Offset="1.0" Color="Navy" />
        </LinearGradientBrush>
    </Application.Resources>
</Application>
