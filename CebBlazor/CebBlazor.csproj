<Project Sdk="Microsoft.NET.Sdk.Web">
    <PropertyGroup>
        <TargetFramework>net7.0</TargetFramework>
        <Nullable>enable</Nullable>
        <LangVersion>preview</LangVersion>
        <IsPackable>true</IsPackable>
        <ImplicitUsings>enable</ImplicitUsings>
        <TypeScriptEnabled>false</TypeScriptEnabled>
        <TypeScriptCompileBlocked>false</TypeScriptCompileBlocked>
        <TypeScriptCompileOnSaveEnabled>False</TypeScriptCompileOnSaveEnabled>
        <TypeScriptSourceMap>False</TypeScriptSourceMap>
        <TypeScriptNoEmitOnError>False</TypeScriptNoEmitOnError>
        <NoWarn>1701;1702;8002</NoWarn>
        <UserSecretsId>863459e9-e5ca-4fdd-813d-9a4f7efc30bc</UserSecretsId>
        <AssemblyVersion>4.8.1.24887</AssemblyVersion>
        <FileVersion>4.8.1.24887</FileVersion>
        <Version>4.2.2.20090</Version>
        <Copyright>©️Arnaud Morin</Copyright>
        <Authors>@Arnaud Morin</Authors>
        <SignAssembly>False</SignAssembly>
    </PropertyGroup>

    <ItemGroup>
        <TypeScriptCompile Remove="node_modules\**" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Microsoft.Web.LibraryManager.Build" Version="2.1.175" />
        <PackageReference Include="MongoDB.Driver" Version="2.17.0" />
        <PackageReference Include="Syncfusion.Blazor.Core" Version="20.2.0.39" />
        <PackageReference Include="Syncfusion.Blazor.Grid" Version="20.2.0.39" />
        <PackageReference Include="Syncfusion.Blazor.ProgressBar" Version="20.2.0.39" />
    </ItemGroup>

    <ItemGroup>
        <Folder Include="wwwroot\css\" />
        <Folder Include="wwwroot\js\fontawesome\" />
        <Folder Include="wwwroot\js\" />
        <Folder Include="wwwroot\webfonts\" />
    </ItemGroup>

    <ItemGroup>
        <ProjectReference Include="..\CompteEstBon\CompteEstBon.csproj" />
        <ProjectReference Include="..\SFCebOffice\SFCebOffice.csproj" />
    </ItemGroup>

    <ProjectExtensions>
        <VisualStudio>
            <UserProperties appsettings_1development_1json__JsonSchema="https://appsemble.app/api.json"
                            libman_1json__JsonSchema="" package_1json__JsonSchema="" />
        </VisualStudio>
    </ProjectExtensions>
</Project>