<Project Sdk="Microsoft.NET.Sdk.Web">
  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
    <LangVersion>9.0</LangVersion>
    <UserSecretsId>da7aee05-0b46-45e9-ae66-4acbcec746e2</UserSecretsId>
    <RunAnalyzersDuringBuild>false</RunAnalyzersDuringBuild>
    <RunAnalyzersDuringLiveAnalysis>false</RunAnalyzersDuringLiveAnalysis>
    <AssemblyName>CebBlazor</AssemblyName>
    <RootNamespace>CompteEstBon</RootNamespace>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>arnaudmorin.snk</AssemblyOriginatorKeyFile>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Extensions.ApiDescription.Client" Version="5.0.0-rc.1.20451.17">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="Microsoft.Web.LibraryManager.Build" Version="2.1.76" />
    <PackageReference Include="Syncfusion.Blazor" Version="18.2.0.58" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="wwwroot\css\" />
    <Folder Include="wwwroot\lib\fontawesome\" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CompteEstBon5\CompteEstBon5.csproj" />
    <ProjectReference Include="..\SFCebOffice\SFCebOffice.csproj" />
  </ItemGroup>

  <ProjectExtensions>
    <VisualStudio><UserProperties libman_1json__JsonSchema="" package_1json__JsonSchema="" properties_4launchsettings_1json__JsonSchema="" /></VisualStudio>
  </ProjectExtensions>

  <Target Name="PreBuild" BeforeTargets="PreBuildEvent" Condition=" '$(Configuration)'=='Debug' ">
    <Exec Command="yarn run sass-debug" />
  </Target>
   <Target Name="PreBuild" BeforeTargets="PreBuildEvent" Condition=" '$(Configuration)'=='Release' ">
    <Exec Command="yarn run sass-debug" />
  </Target>
</Project>
