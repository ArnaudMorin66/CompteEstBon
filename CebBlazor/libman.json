{
  "version": "1.0",
  "defaultProvider": "filesystem",
  "libraries": [
    {
      "library": "node_modules/@syncfusion/ej2",
      "destination": "scss/",
      "files": [
        "material.css",
        "bootstrap4.css",
        "fabric.css"
      ]
    },
    {
      "library": "node_modules/@syncfusion/ej2/dist",
      "destination": "wwwroot/lib/ej2/js/",
      "files": [
        "ej2.min.js"
      ]
    },
    {
      "library": "C:/Apps/ej2/js",
      "destination": "wwwroot/lib/ej2/js/",
      "files": [
        "ejs.interop.min.js"
      ]
    },
    {
      "library": "node_modules/@fortawesome/fontawesome-free/js",
      "destination": "wwwroot/lib/fontawesome/js/",
      "files": [
        "all.min.js"
      ]
    },
    {
      "library": "node_modules/jquery/dist/",
      "destination": "wwwroot/lib/jquery/",
      "files": [
        "jquery.min.js"
      ]
    }
]
}