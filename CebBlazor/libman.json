{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@5.8.2",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "twitter-bootstrap@4.3.1",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "popper.js@1.15.0",
      "destination": "wwwroot/lib/popper.js/"
    },
    {
      "library": "jquery@3.4.1",
      "destination": "wwwroot/lib/jquery/"
    },
    {
      "provider": "unpkg",
      "library": "@progress/kendo-theme-default@latest",
      "destination": "wwwroot/lib/@progress/kendo-theme-default/",
      "files": [
        "dist/all.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@progress/kendo-theme-material@latest",
      "destination": "wwwroot/lib/@progress/kendo-theme-material/",
      "files": [
        "dist/all.css"
      ]
    },
    {
      "provider": "unpkg",
      "library": "@progress/kendo-theme-bootstrap@latest",
      "destination": "wwwroot/lib/@progress/kendo-theme-bootstrap/",
      "files": [
        "dist/all.css"
      ]
    }
  ]
}