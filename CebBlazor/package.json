{
  "name": "cebblazor",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "sass-debug":
      "sass scss/site.scss:scss/debug/site.css -I node_modules/ -I node_modules/@syncfusion/ --source-map -c",
    "sass-release":
      "sass scss/site.scss:scss/release/site.min.css --style=compressed -I node_modules/ -I node_modules/@syncfusion/ -c --no-source-map",
    "sass-watch":
      "sass scss/site.scss:wwwroot/css/site.min.css --style=compressed -I node_modules/ -I node_modules/@syncfusion/ -c --no-source-map -w"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.2.1",
    "@syncfusion/ej2": "^20.4.42",
    "bootstrap": "^5.2.3"
  },
  "devDependencies": {
    "sass": "1.57.1",
    "stylelint-config-twbs-bootstrap": "^7.0.0"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "sass-watch"
    ]
  }
}