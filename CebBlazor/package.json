{
  "name": "cebblazor",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "sass-debug": "sass scss/site.scss:scss/debug/site.css -I node_modules/ -I node_modules/@syncfusion/ --source-map -c",
    "sass-release": "sass scss/site.scss:scss/release/site.min.css --style=compressed -I node_modules/ -I node_modules/@syncfusion/ -c --no-source-map",
    "sass-watch": "sass scss/site.scss:wwwroot/css/site.min.css --style=compressed -I node_modules/ -I node_modules/@syncfusion/ -c --no-source-map -w"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "6.5.2",
    "@syncfusion/ej2": "25.2.7",
    "bootstrap": "5.3.3"
  },
  "devDependencies": {
    "sass": "1.69.5",
    "stylelint-config-twbs-bootstrap": "12.0.0"
  },
  "-vs-binding": {
    "ProjectOpened": [
      "sass-watch"
    ]
  }
}
