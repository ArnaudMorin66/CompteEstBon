<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <UseWPF>true</UseWPF>
    <RootNamespace>CompteEstBon</RootNamespace>
    <StartupObject>CompteEstBon.App</StartupObject>
    <ApplicationIcon>favicon.ico</ApplicationIcon>
    <AssemblyName>Ceb</AssemblyName>
  </PropertyGroup>

  <ItemGroup>
    <Compile Remove="ViewModel\InteropClass.cs" />
    <Compile Remove="xMainWindow.xaml.cs" />
  </ItemGroup>

  <ItemGroup>
    <None Remove="favicon.ico" />
    <None Remove="Images\excel.png" />
    <None Remove="Images\exec.png" />
    <None Remove="Images\favicon.png" />
    <None Remove="Images\random.png" />
    <None Remove="Images\word.png" />
  </ItemGroup>

  <ItemGroup>
    <Page Remove="xMainWindow.xaml" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Syncfusion.SfGrid.WPF" Version="17.4.0.50" />
    <PackageReference Include="Syncfusion.SfInput.WPF" Version="17.4.0.50" />
    <PackageReference Include="Syncfusion.Themes.Blend.WPF" Version="17.4.0.50" />
    <PackageReference Include="Syncfusion.Themes.SystemTheme.WPF" Version="17.4.0.50" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\CebOfficeCore\CebOfficeCore.csproj" />
    <ProjectReference Include="..\CompteEstBon_2.1\CompteEstBon.core.csproj" />
  </ItemGroup>

  <ItemGroup>
    <Resource Include="favicon.ico" />
    <Resource Include="Images\excel.png" />
    <Resource Include="Images\exec.png" />
    <Resource Include="Images\favicon.png" />
    <Resource Include="Images\random.png" />
    <Resource Include="Images\word.png" />
  </ItemGroup>

  <ItemGroup>
    <Compile Update="Properties\Settings.Designer.cs">
      <DesignTimeSharedInput>True</DesignTimeSharedInput>
      <AutoGen>True</AutoGen>
      <DependentUpon>Settings.settings</DependentUpon>
    </Compile>
  </ItemGroup>

  <ItemGroup>
    <None Update="Properties\Settings.settings">
      <Generator>SettingsSingleFileGenerator</Generator>
      <LastGenOutput>Settings.Designer.cs</LastGenOutput>
    </None>
    <None Update="SyncfusionLicense.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

  <ItemGroup>
    <Page Update="CebDictionary.xaml">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
      <SubType>Designer</SubType>
    </Page>
  </ItemGroup>

</Project>